      --input-cursor=*) COMPREPLY=($(compgen -W "--input-cursor=yes --input-cursor=no" -- "$cur")) ; return ;; 
      --input-default-bindings=*) COMPREPLY=($(compgen -W "--input-default-bindings=yes --input-default-bindings=no" -- "$cur")) ; return ;; 
