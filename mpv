      --keepaspect=*) COMPREPLY=($(compgen -W "--keepaspect=yes --keepaspect=no" -- "$cur")) ; return ;; 
      --keepaspect-window=*) COMPREPLY=($(compgen -W "--keepaspect-window=yes --keepaspect-window=no" -- "$cur")) ; return ;; 
      --load-scripts=*) COMPREPLY=($(compgen -W "--load-scripts=yes --load-scripts=no" -- "$cur")) ; return ;; 
      --load-unsafe-playlists=*) COMPREPLY=($(compgen -W "--load-unsafe-playlists=yes --load-unsafe-playlists=no" -- "$cur")) ; return ;; 
