    return
  fi
  compopt -o filenames -o default
  _filedir
}
complete -o nospace -F _mpv mpv
if [[ -n $BASH_VERSINFO && $BASH_VERSINFO -eq 3 ]] ; then
  echo "[$0] WARNING: This script is not compatible with Bash 3.x!">&2
