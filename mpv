      --hwdec-preload=*) COMPREPLY=($(compgen -W "--hwdec-preload=no --hwdec-preload=auto --hwdec-preload=vdpau --hwdec-preload=videotoolbox --hwdec-preload=vaapi --hwdec-preload=vaapi-copy --hwdec-preload=dxva2 --hwdec-preload=dxva2-copy --hwdec-preload=d3d11va-copy --hwdec-preload=rpi --hwdec-preload=mediacodec" -- "$cur")) ; return ;; 
