#!/bin/bash
# mpv(1) completion

_mpv(){
  local cur=${COMP_WORDS[COMP_CWORD]}
  local prev=${COMP_WORDS[COMP_CWORD-1]}
  COMP_WORDBREAKS=${COMP_WORDBREAKS/=/}
  compopt +o default +o filenames
  if [[ -n $cur ]] ; then
    case "$cur" in
      --ad-lavc-downmix=*) COMPREPLY=($(compgen -W "--ad-lavc-downmix=yes --ad-lavc-downmix=no" -- "$cur")) ; return ;; 
      --ad-spdif-dtshd=*) COMPREPLY=($(compgen -W "--ad-spdif-dtshd=yes --ad-spdif-dtshd=no" -- "$cur")) ; return ;; 
      --aid=*) COMPREPLY=($(compgen -W "--aid=no --aid=auto --aid=0 --aid=8190" -- "$cur")) ; return ;; 
      --ass-force-margins=*) COMPREPLY=($(compgen -W "--ass-force-margins=yes --ass-force-margins=no" -- "$cur")) ; return ;; 
      --ass-hinting=*) COMPREPLY=($(compgen -W "--ass-hinting=none --ass-hinting=light --ass-hinting=normal --ass-hinting=native" -- "$cur")) ; return ;; 
      --ass-scale-with-window=*) COMPREPLY=($(compgen -W "--ass-scale-with-window=yes --ass-scale-with-window=no" -- "$cur")) ; return ;; 
      --ass-shaper=*) COMPREPLY=($(compgen -W "--ass-shaper=simple --ass-shaper=complex" -- "$cur")) ; return ;; 
      --ass-style-override=*) COMPREPLY=($(compgen -W "--ass-style-override=no --ass-style-override=yes --ass-style-override=force --ass-style-override=signfs" -- "$cur")) ; return ;; 
      --ass-vsfilter-aspect-compat=*) COMPREPLY=($(compgen -W "--ass-vsfilter-aspect-compat=yes --ass-vsfilter-aspect-compat=no" -- "$cur")) ; return ;; 
      --ass-vsfilter-blur-compat=*) COMPREPLY=($(compgen -W "--ass-vsfilter-blur-compat=yes --ass-vsfilter-blur-compat=no" -- "$cur")) ; return ;; 
      --ass-vsfilter-color-compat=*) COMPREPLY=($(compgen -W "--ass-vsfilter-color-compat=no --ass-vsfilter-color-compat=basic --ass-vsfilter-color-compat=full --ass-vsfilter-color-compat=force-601" -- "$cur")) ; return ;; 
      --audio-display=*) COMPREPLY=($(compgen -W "--audio-display=no --audio-display=attachment" -- "$cur")) ; return ;; 
      --audio-fallback-to-null=*) COMPREPLY=($(compgen -W "--audio-fallback-to-null=yes --audio-fallback-to-null=no" -- "$cur")) ; return ;; 
      --audio-file-auto=*) COMPREPLY=($(compgen -W "--audio-file-auto=no --audio-file-auto=exact --audio-file-auto=fuzzy --audio-file-auto=all" -- "$cur")) ; return ;; 
      --audio-normalize-downmix=*) COMPREPLY=($(compgen -W "--audio-normalize-downmix=yes --audio-normalize-downmix=no" -- "$cur")) ; return ;; 
      --audio-pitch-correction=*) COMPREPLY=($(compgen -W "--audio-pitch-correction=yes --audio-pitch-correction=no" -- "$cur")) ; return ;; 
      --autosync=*) COMPREPLY=($(compgen -W "--autosync=no --autosync=0 --autosync=10000" -- "$cur")) ; return ;; 
      --border=*) COMPREPLY=($(compgen -W "--border=yes --border=no" -- "$cur")) ; return ;; 
      --cache=*) COMPREPLY=($(compgen -W "--cache=no --cache=auto --cache=yes --cache=32 --cache=2147483647" -- "$cur")) ; return ;; 
      --cache-default=*) COMPREPLY=($(compgen -W "--cache-default=no --cache-default=32 --cache-default=2147483647" -- "$cur")) ; return ;; 
      --cache-pause=*) COMPREPLY=($(compgen -W "--cache-pause=yes --cache-pause=no" -- "$cur")) ; return ;; 
      --cdda-cdtext=*) COMPREPLY=($(compgen -W "--cdda-cdtext=yes --cdda-cdtext=no" -- "$cur")) ; return ;; 
      --cdda-skip=*) COMPREPLY=($(compgen -W "--cdda-skip=yes --cdda-skip=no" -- "$cur")) ; return ;; 
      --config=*) COMPREPLY=($(compgen -W "--config=yes --config=no" -- "$cur")) ; return ;; 
      --cookies=*) COMPREPLY=($(compgen -W "--cookies=yes --cookies=no" -- "$cur")) ; return ;; 
      --correct-pts=*) COMPREPLY=($(compgen -W "--correct-pts=yes --correct-pts=no" -- "$cur")) ; return ;; 
      --cursor-autohide=*) COMPREPLY=($(compgen -W "--cursor-autohide=no --cursor-autohide=always --cursor-autohide=0 --cursor-autohide=30000" -- "$cur")) ; return ;; 
      --cursor-autohide-fs-only=*) COMPREPLY=($(compgen -W "--cursor-autohide-fs-only=yes --cursor-autohide-fs-only=no" -- "$cur")) ; return ;; 
      --deinterlace=*) COMPREPLY=($(compgen -W "--deinterlace=auto --deinterlace=no --deinterlace=yes" -- "$cur")) ; return ;; 
      --demuxer-lavf-allow-mimetype=*) COMPREPLY=($(compgen -W "--demuxer-lavf-allow-mimetype=yes --demuxer-lavf-allow-mimetype=no" -- "$cur")) ; return ;; 
